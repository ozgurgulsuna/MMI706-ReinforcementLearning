<!-- Define joints -->
	<joint name="(1)" type="prismatic">
		<parent link="[1-0]"/>
		<child link="[1-1]"/>
		<origin rpy="0 0 0" xyz="0 0 0.5"/>
		<axis xyz="0 0 1"/>
		<limit effort="100" lower="-0" upper="1" velocity="1"/>
	</joint>

	<joint name="(1-S)" type="fixed">
		<parent link="[1-0]"/>
		<child link="[1-S]"/>
		<origin rpy="0 0 0" xyz="0 0 0"/>
	</joint>

	<joint name="(2)" type="prismatic">
		<parent link="[2-0]"/>
		<child link="[2-1]"/>
		<origin rpy="0 0 0" xyz="0 0 0.5"/>
		<axis xyz="0 0 1"/>
		<limit effort="100" lower="-0" upper="1" velocity="1"/>
	</joint>

	<joint name="(2-S)" type="fixed">
		<parent link="[2-0]"/>
		<child link="[2-S]"/>
		<origin rpy="0 0 0" xyz="0 0 0"/>
	</joint>

	<joint name="(1-2)" type="floating">
		<parent link="[1-1]"/>
		<child link="[2-0]"/>
		<origin rpy="0 0 0" xyz="0 0 0.5"/>
	</joint>

	<joint name="(3)" type="prismatic">
		<parent link="[3-0]"/>
		<child link="[3-1]"/>
		<origin rpy="0 0 0" xyz="0 0 0.5"/>
		<axis xyz="0 0 1"/>
		<limit effort="100" lower="-0" upper="1" velocity="1"/>
