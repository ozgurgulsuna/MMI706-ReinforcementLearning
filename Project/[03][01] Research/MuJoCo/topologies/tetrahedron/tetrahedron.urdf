<?xml version="1.0" ?>
<robot name="tetrahedron">
<!--https://en.wikipedia.org/wiki/Tetrahedron-->

<!-- Define materials -->
	<material name="gray"> 
		<color rgba="0.5 0.5 0.5 1"/>
	</material>

	<material name="red">
		<color rgba="1 0 0 1"/>
	</material>

<!-- Define links -->
<!-- Member 1 -->
	<link name="[1-0]">
		<visual>
			<geometry>
				<cylinder length="1" radius="0.05"/>
				<!-- <mesh filename="package://tetrahedron/meshes/tetrahedron.stl"/> -->
			</geometry>
			<origin rpy="0 0 0" xyz="0 0 0.5"/>
			<material name="gray"/>
		</visual>

		<collision>
			<geometry>
				<cylinder length="1" radius="0.05"/>
			</geometry>
			<origin rpy="0 0 0" xyz="0 0 0.5"/>
		</collision>

		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0.5"/>
			<mass value="1"/>
			<inertia 
			ixx="0.1" ixy="0" ixz="0" 
			iyy="0.1" iyz="0" izz="0.1"
			/>
		</inertial>
	</link>

	<link name="[1-S]">
		<visual>
			<geometry>
				<sphere radius="0.1"/>
			</geometry>
			<origin rpy="0 0 0" xyz="0 0 0.05"/>
			<material name="red"/>
		</visual>

		<collision>
			<geometry>
				<sphere radius="0.1"/>
			</geometry>
			<origin rpy="0 0 0" xyz="0 0 0.05"/>
		</collision>

		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0.05"/>
			<mass value="1"/>
			<inertia 
			ixx="0.1" ixy="0" ixz="0" 
			iyy="0.1" iyz="0" izz="0.1"
			/>
		</inertial>
	</link>


	<link name="[1-1]">
		<visual>
			<geometry>
				<cylinder length="1" radius="0.04"/>
			</geometry>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<material name="gray"/>
		</visual>

		<collision>
			<geometry>
				<cylinder length="1" radius="0.04"/>
			</geometry>
			<origin rpy="0 0 0" xyz="0 0 0"/>
		</collision>

		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<mass value="1"/>
			<inertia 
			ixx="0.1" ixy="0" ixz="0" 
			iyy="0.1" iyz="0" izz="0.1"
			/>
		</inertial>
	</link>

