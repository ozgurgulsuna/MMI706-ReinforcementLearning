	<joint name="(3-S)" type="fixed">
		<parent link="[3-0]"/>
		<child link="[3-S]"/>
		<origin rpy="0 0 0" xyz="0 0 0"/>
	</joint>

	<joint name="(2-3)" type="floating">
		<parent link="[2-1]"/>
		<child link="[3-0]"/>
		<origin rpy="0 0 0" xyz="0 0 0.5"/>
	</joint>

	<joint name="(3-1)" type="floating">
		<parent link="[3-1]"/>
		<child link="[1-0]"/>
		<origin rpy="0 0 0" xyz="0 0 0.5"/>
	</joint>
	

</robot>